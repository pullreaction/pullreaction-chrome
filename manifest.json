{
    "manifest_version": 2,
    "name": "Pull Reaction",
    "version": "0.0.1",

    "background" : {
        "persistent": false,
        "scripts": [
            "js/init.js"
        ]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"]
        }
    ],
    "permissions": [
        "contextMenus"
    ]
}